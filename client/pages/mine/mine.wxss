page {  
  display: block;  
  min-height: 100%;  
  background-color: #f8f6fb;  
}  

.contain{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  background: #ffffff;
  border: 1px solid rgba(0, 0, 0, .1);
}

.button{
    width: 280rpx;
    height: 80rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    margin-right: 15rpx
}

.counter{
  background: #ffffff;
  margin: 20rpx 0 0 0;
  padding: 17rpx 35rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  font-size: 36rpx;
  border-bottom: 1px #eaeaed solid;
  border-top: 1px #eaeaed solid;
}
.userinfo, .uploader, .tunnel {
    height: 140rpx;
    width: 100%;
    background: #FFF;
    border-left: none;
    border-right: none;
    display: flex;
    flex-direction: row;
    align-items: center;
    transition: all 300ms ease;
}
.uploader-container {
    width: 100%;
    height: 400rpx;
    padding: 20rpx 20rpx 20rpx 0;
    display: flex;
    align-content: center;
    justify-content: center;
    box-sizing: border-box;
    border-top: 1px solid rgba(0, 0, 0, .1);
}
.userinfo-avatar {
    width: 100rpx;
    height: 100rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    font-size: 32rpx;
    color: #007AFF;
    background-color: white; 
}

.userinfo-nickname::after {
  border: none;
}
